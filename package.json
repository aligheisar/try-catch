{
  "name": "@aligheisar/try-catch",
  "version": "0.1.0",
  "description": "A lightweight TypeScript utility to handle async Promises with typed try/catch results.",
  "author": {
    "email": "ali.gheisar.dev@gmail.com",
    "name": "Ali Gheisar",
    "url": "https://github.com/aligheisar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aligheisar/try-catch"
  },
  "homepage": "https://github.com/aligheisar/try-catch",
  "keywords": [
    "promise",
    "error-handling",
    "async",
    "try-catch",
    "error",
    "api",
    "typescript",
    "javascript",
    "utilities"
  ],
  "license": "ISC",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "format": "biome format --write",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.24.0"
}
